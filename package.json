{
  "name": "green-analytics",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap && npx shx cp -r ./node_modules/green-analytics-js/build/index.js ./public/green-analytics.js",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "biome format --write . && biome lint --apply .",
    "lint:staged": "lint-staged",
    "db:update": "prisma db push && prisma generate --data-proxy",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "typescript:check": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "biome format --write",
    "*.{js,jsx,ts,tsx|json}": "biome lint --apply"
  },
  "sideEffects": false,
  "resolutions": {
    "@mui/material": "npm:@mui/joy@latest"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^3.3.0",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@makakwastaken/co2": "^0.12.4",
    "@mui/icons-material": "^5.14.18",
    "@mui/joy": "^5.0.0-beta.15",
    "@mui/material": "npm:@mui/joy@latest",
    "@next/bundle-analyzer": "^14.0.3",
    "@prisma/client": "^5.6.0",
    "axios": "^1.6.2",
    "chart.js": "^4.4.0",
    "convert": "^4.14.0",
    "green-analytics-js": "^1.0.12",
    "luxon": "^3.4.4",
    "next": "14.0.3",
    "next-sitemap": "^4.2.3",
    "nextjs-cors": "^2.1.2",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "18.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-toastify": "^9.1.3",
    "request-country": "^0.1.6",
    "sharp": "^0.32.6",
    "swr": "^2.2.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.3.3",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@lhci/cli": "^0.12.0",
    "@types/luxon": "^3.3.5",
    "@types/node": "^20.9.4",
    "@types/react": "^18.2.38",
    "@types/react-dom": "18.2.17",
    "@types/react-syntax-highlighter": "^15.5.10",
    "@types/url-file-size": "^1.0.3",
    "@types/uuid": "^9.0.7",
    "babel-plugin-import": "^1.13.8",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prisma": "^5.6.0",
    "typescript": "^5.3.2"
  }
}